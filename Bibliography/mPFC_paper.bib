issn = {14609568},
journal = {European Journal of Neuroscience},
keywords = {Cortical architectonics,Density profiles,Human,Ventromedial prefrontal cortex},
number = {5},
pages = {2777--2796},
pmid = {25123211},
title = {{Architecture and morphology of the human ventromedial prefrontal cortex}},
volume = {40},
