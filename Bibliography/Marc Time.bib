keywords = {Animals,Association Learning,Hippocampus,Hippocampus: physiology,Humans,Ibotenic Acid,Ibotenic Acid: diagnostic use,Male,Memory,Memory: physiology,Mental Recall,Mental Recall: physiology,Rats,Species Specificity},
pages = {255--7},
pmid = {8538790},
title = {{Conservation of hippocampal memory function in rats and humans.}},
