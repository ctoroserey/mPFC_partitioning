journal = {Philosophical Transactions of the Royal Society B: Biological Sciences},
keywords = {consistency,precision,scalar timing,time perception},
number = {1525},
pages = {1897--1905},
pmid = {19487192},
title = {{The precision of temporal judgement: milliseconds, many minutes, and beyond}},
url = {http://rstb.royalsocietypublishing.org/cgi/doi/10.1098/rstb.2009.0020},
volume = {364},
year = {2009}
}
