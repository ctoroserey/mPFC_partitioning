@article{Nyhus2010,
abstract = {The primary aim of this review is to examine evidence for a functional role of gamma and theta oscillations in human episodic memory. It is proposed here that gamma and theta oscillations allow for the transient interaction between cortical structures and the hippocampus for the encoding and retrieval of episodic memories as described by the hippocampal memory indexing theory (Teyler and DiScenna, 1986). Gamma rhythms can act in the cortex to bind perceptual features and in the hippocampus to bind the rich perceptual and contextual information from diverse brain regions into episodic representations. Theta oscillations act to temporally order these individual episodic memory representations. Through feedback projections from the hippocampus to the cortex these gamma and theta patterns could cause the reinstatement of the entire episodic memory representation in the cortex. In addition, theta oscillations could allow for top-down control from the frontal cortex to the hippocampus modulating the encoding and retrieval of episodic memories. {\textcopyright} 2009.},
annote = {This one provides a nice overview. The main points are:

Both theta and gamma oscillations have been related to the encoding (and to some extent, retrieval) of episodic memories. Theta has been reported in both hippocampus and PFC, and is present for synchronized activity throughout long ranges of cortex, while gamma is mainly local. What Nyhus and Curran propose is that during encoding, theta phase precession works as a temporal organizer in the same way that place cells use such precession. This means that representations of memories, guided by gamma activity in hippocampus, become related to a specific phase in theta oscillations, such that continuous episodes are linked to theta's precession in time.

Theta, they argue, also provides a mechanism for top-down control from mPFC to hippocampus, which could act as a contextual retrieval tool. 

In the end, what's important here is that successful encoding of temporally contiguous stimuli relies on neuronal oscillatory activity bounded at the theta range.

Interestingly, they also talk about how alpha (8-12hz) has been reported to underlie semantic relatedness in memory experiments.},
author = {Nyhus, Erika and Curran, Tim},
doi = {10.1016/j.neubiorev.2009.12.014},
file = {:Users/Claudio/Library/Application Support/Mendeley Desktop/Downloaded/Nyhus, Curran - 2010 - Functional role of gamma and theta oscillations in episodic memory(2).pdf:pdf},
isbn = {0149-7634},
issn = {01497634},
journal = {Neuroscience and Biobehavioral Reviews},
keywords = {Episodic memory,Gamma,Oscillations,Theta},
number = {7},
pages = {1023--1035},
pmid = {20060015},
publisher = {Elsevier Ltd},
title = {{Functional role of gamma and theta oscillations in episodic memory}},
url = {http://dx.doi.org/10.1016/j.neubiorev.2009.12.014},
volume = {34},
year = {2010}
}
@article{Cole1995,
abstract = {Conditioned lick suppression in rats was used to explore the role of timing in trace conditioning. In Experiment 1, two groups of rats were exposed to pairings of a CS (CS1) with a US, under conditions in which the interstimulus interval (ISI) that separated CS1 offset and US onset was either 0 or 5 sec. Two additional groups were also exposed to the same CS1--{\textgreater}US pairings with either a 0 or a 5-sec ISI, and then received ''backward'' second-order conditioning in which CS1 was immediately followed by a novel CS2 (i.e., CS1--{\textgreater}CS2). A trace conditioning deficit was observed in that the CS1 conditioned with the 5-sec gap supported less excitatory responding than the CS1 conditioned with the 0-sec gap. However, CS2 elicited more conditioned responding in the group trained with the 5-sec CS1-US gap than in the group trained with the 0-sec CS1-US gap. Thus, the CS1-US interval had inverse effects on first- and second-order conditioned responding. Experiment 2 was conducted as a sensory preconditioning analogue to Experiment 1. In Experiment 2, rats received the CS1--{\textgreater}CS2 pairings prior to the CS1--{\textgreater}US pairings (in which CS1 was again conditioned with either a 0 or a 5-sec ISI). Experiment 2 showed a dissociation between first- and second-order conditioned responding similar to that observed in Experiment 1. These outcomes are not compatible with the view that differences in responding to CSs conditioned with different ISIs are mediated exclusively by differences in associative value. The results are discussed in the framework of the temporal coding hypothesis, according to which temporal relationships between events are encoded in elementary associations.},
author = {Cole, Robert P. and Barnet, Robert C. and Miller, Ralph R.},
doi = {10.3758/BF03199929},
file = {:Users/Claudio/Library/Application Support/Mendeley Desktop/Downloaded/Cole, Barnet, Miller - 1995 - Temporal encoding in trace conditioning(2).pdf:pdf},
isbn = {0090-4996},
issn = {00904996},
journal = {Animal Learning {\&} Behavior},
number = {2},
pages = {144--153},
title = {{Temporal encoding in trace conditioning}},
volume = {23},
year = {1995}
}
