isbn = {1069-9384},
issn = {1069-9384},
journal = {Psychonomic Bulletin {\&} Review},
number = {3},
pages = {382--389},
pmid = {24203522},
title = {{Temporal discounting and preference reversals in choice between delayed outcomes.}},
url = {http://link.springer.com/article/10.3758/BF03213979{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/24203522},
