issn = {1522-1598},
journal = {J Neurophysiol Journal of Neurophysiology},
number = {21},
pages = {1507--1523},
title = {{Human Ventral Striatum Short-Term Temporal Discounting of Reward Value in Short-Term Temporal Discounting of Reward Value in Human Ventral Striatum}},
url = {http://jn.physiology.org/content/101/3/1507.full{\#}ref-list-1 http://jn.physiology.org/content/101/3/1507{\#}cited-by http://jn.physiology.org/content/101/3/1507.full http://www.the-aps.org/. http://jn.physiology.org/ http://jn.physiology.org/content/101/3/150},
